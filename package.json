{
  "name": "cluster-info",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run dev:server & npm run dev:frontend",
    "dev:server": "POD_NAME=local-pod POD_NAMESPACE=local-namespace NODE_NAME=local-node CLUSTER_NAME=local-cluster APP_VERSION=0.1.0 PORT=3001 tsx server/index.ts",
    "dev:frontend": "vite",
    "build": "vite build",
    "preview": "tsx server/index.ts",
    "start": "tsx server/index.ts",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "prepare": "husky"
  },
  "dependencies": {
    "express": "^4.21.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.6",
    "postcss": "^8.4.47",
    "prettier": "^3.8.0",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.4.6",
    "tsc-files": "^1.1.4",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.3.1"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "tsc-files --noEmit",
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.{json,md,yaml,yml}": [
      "prettier --write"
    ]
  }
}
